
public class TwoWheeler extends Vechile {

	private boolean kickStartAvailable;
	
	
	

	public TwoWheeler(String make, String vechileNumber, String fueltype, int fuelcapacaity, int cc,
			boolean kickStartAvailable) {
		super(make, vechileNumber, fueltype, fuelcapacaity, cc);
		this.kickStartAvailable = kickStartAvailable;
	}

	public boolean isKickStartAvailable() {
		return kickStartAvailable;
	}

	public void setKickStartAvailable(boolean kickStartAvailable) {
		this.kickStartAvailable = kickStartAvailable;
	}
	
	public void displayDetailInfo() {
		
		System.out.println("Kickstart Available(Yes/No):"+isKickStartAvailable());
	}
}
